{
    "name": "prox/prox-gallery",
    "version": "0.1.0",
    "description": "An open source gallery",
    "type": "wordpress-plugin",
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml.dist",
        "test:isolated:init": "WP_PHPUNIT__TESTS_CONFIG=./tests/wp-tests-config.php ./vendor/bin/phpunit -c phpunit.xml.dist",
        "test:isolated": "WP_PHPUNIT__TESTS_CONFIG=./tests/wp-tests-config.php WP_TESTS_SKIP_INSTALL=1 ./vendor/bin/phpunit -c phpunit.xml.dist",
        "test:list": "./vendor/bin/phpunit -c phpunit.xml.dist --list-tests",
        "test:dox": "./vendor/bin/phpunit -c phpunit.xml.dist --testdox",
        "analyse": "./vendor/bin/phpstan analyse",
        "fix": "./vendor/bin/php-cs-fixer fix"
    },
    "require": {
        "php": "^8.1",
        "automattic/jetpack-autoloader": "^5.0",
        "composer/installers": "^2.3",
        "webmozart/assert": "^1.11",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.94",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.6",
        "yoast/phpunit-polyfills": "^2.0"
    },
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Prox\\ProxGallery\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Marcel Santing",
            "email": "marcel@prox-web.nl"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "automattic/jetpack-autoloader": true,
            "composer/installers": true
        }
    }
}
